<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.cellview.client">
<ui:style>

	</ui:style>
<g:HTMLPanel ui:field="rootPanel">
	<g:VerticalPanel width="824px" styleName="center">
	<!-- NAV STUFF -->
	<g:Cell horizontalAlignment="ALIGN_CENTER">
		<g:HorizontalPanel>
			<g:Image
				url="https://www.wpi.edu/sites/default/files/inline-image/Offices/Marketing-Communications/WPI_Inst_Prim_FulClr_PREVIEW.png"
				height="5%" />
			<g:Button text="Home" styleName="navButton" ui:field="homeButton"
				height="38px" width="56px"></g:Button>
			<g:Button text="Schedule" styleName="navButton" ui:field="scheduleButton"
				height="38px"></g:Button>
			<g:Button text="Courses" styleName="navButton" ui:field="coursesButton"
				height="38px"></g:Button>
			<g:Button text="Account" styleName="navButton" ui:field="accountButton"
				height="38px"></g:Button>
		</g:HorizontalPanel>
	</g:Cell>
	<g:Label text="Welcome!" width="355px" styleName="page-title" />
	<g:VerticalPanel ui:field="facultyListVerticalPanel">
		<g:HorizontalPanel ui:field="facultyListTitlePanel">
			<g:Label text="Number of Courses" styleName="faculty-list-title" ui:field="numberOfCoursesLabel"/>
			<g:Label text="Faculty Name" styleName="faculty-list-title" ui:field="facultyNameLabel"/>
			<g:Label text="Assigned Courses" styleName="faculty-list-title" ui:field="assignedCoursesLabel"/>
		</g:HorizontalPanel>
	</g:VerticalPanel>
	</g:VerticalPanel>
</g:HTMLPanel>
</ui:UiBinder> 