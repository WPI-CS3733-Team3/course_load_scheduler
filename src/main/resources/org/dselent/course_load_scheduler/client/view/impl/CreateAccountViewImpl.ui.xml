<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>

	</ui:style>
	<g:VerticalPanel styleName="center" width="450" height="300" ui:field="createAccountPanel" horizontalAlignment="ALIGN_LEFT">
		<g:Image height="20%"
			url="WEB-INF/classes/org/dselent/course_load_scheduler/client/view/images/WPI_Inst_Prim_FulClr_med.png"
			width="" />
		<g:Label text="Create Account" ui:field="createAccountLabel"
			styleName="login-title" />
		<g:VerticalPanel>
			<g:Grid>
				<g:row>
					<g:customCell>
						<g:HorizontalPanel>
							<g:VerticalPanel>
								<g:Label text="First name:" ui:field="firstNameLabel" />
								<g:TextBox ui:field="firstNameTextBox" width="173px" />
							</g:VerticalPanel>
							<g:VerticalPanel>
								<g:Label text="Last name:" ui:field="lastNameLabel" />
								<g:TextBox ui:field="lastNameTextBox" />
							</g:VerticalPanel>
						</g:HorizontalPanel>
					</g:customCell>
				</g:row>
			</g:Grid>
		</g:VerticalPanel>
		<g:Label text="Faculty type:" ui:field="facultyTypeLabel" />
		<g:ListBox ui:field="facultyTypeComboBox" width="356px" />
		<g:Label text="Email:" ui:field="emailLabel" />
		<g:TextBox ui:field="emailTextBox" width="356px" />
		<g:HorizontalPanel>
			<g:VerticalPanel>
				<g:Label text="Password:" ui:field="passwordLabel" />
				<g:PasswordTextBox ui:field="passwordTextBox"
					width="173px" />
			</g:VerticalPanel>
			<g:VerticalPanel>
				<g:Label text="Confirm password:" ui:field="confirmPasswordLabel" />
				<g:PasswordTextBox ui:field="confirmPasswordTextBox" width="173px"/>
			</g:VerticalPanel>
		</g:HorizontalPanel>
		<g:Cell horizontalAlignment="ALIGN_RIGHT">
			<g:Button ui:field="submitButton">Submit</g:Button>
		</g:Cell>
	</g:VerticalPanel>
</ui:UiBinder> 