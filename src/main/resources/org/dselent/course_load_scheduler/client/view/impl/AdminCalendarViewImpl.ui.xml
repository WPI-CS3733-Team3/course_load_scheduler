<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<g:HTMLPanel ui:field="baseContainer">
		<g:VerticalPanel width="100%" height="100%">
			<g:Cell height="15%">
				<g:SimplePanel height="100%" ui:field="navigationBar">
					<g:Label text="NAVIGATION BAR" width="100%" height="100%" horizontalAlignment="ALIGN_CENTER"/>
				</g:SimplePanel>
			</g:Cell>
			<g:Cell height="15%">
				<g:SimplePanel height="100%" ui:field="pageTitle">
					<g:Label text="Edit Schedule" width="100%" height="100%" horizontalAlignment="ALIGN_CENTER" styleName="page-title"/>
				</g:SimplePanel>
			</g:Cell>
			<g:Cell height="70%">
				<g:HorizontalPanel width="100%" height="100%" ui:field="calendarBody">
					<g:Cell width="14%">
						<g:VerticalPanel ui:field="infoPanel" width="100%">
						<g:Label text="View Schedule For"/>
						<g:ListBox ui:field="viewSelect"/>
						<g:Label text="Academic Year"/>
						<g:ListBox ui:field="yearSelect"/>
						<g:Label text="Term"/>
						<g:ListBox ui:field="termSelect"/>
					</g:VerticalPanel>
					</g:Cell>
					<g:Cell width="1%">
						<g:SimplePanel height="100%" width="1">
						<g:Label width="100%" height="100%" styleName="divider"/>
					</g:SimplePanel>
					</g:Cell>
					<g:Cell width="14%">
						<g:VerticalPanel ui:field="courseInfoPanel" width="100%">
						<g:Label text="Course Information" styleName="section-title"/>
						<g:Label ui:field="courseName" text="course name"/>
						<g:Cell width="100%">
							<g:HorizontalPanel width="100%">
							<g:Cell width="50%" verticalAlignment="ALIGN_MIDDLE">
								<g:Label text="Section" width="50%"/>
							</g:Cell>
							<g:Cell width="50%">
								<g:TextBox text="sec" width="85%"/>
							</g:Cell>
						</g:HorizontalPanel>
						</g:Cell>
						
						<g:Label text="Days"/>
						<g:Cell width="100%">
							<g:HorizontalPanel width="100%">
							<g:Cell width="20%" horizontalAlignment="ALIGN_CENTER">
								<g:Label text="M"/>
							</g:Cell>
							<g:Cell horizontalAlignment="ALIGN_CENTER" width="20%">
								<g:Label text="T"/>
							</g:Cell>
							<g:Cell horizontalAlignment="ALIGN_CENTER" width="20%">
								<g:Label text="W"/>
							</g:Cell>
							<g:Cell horizontalAlignment="ALIGN_CENTER" width="20%">
								<g:Label text="H"/>
							</g:Cell>
							<g:Cell horizontalAlignment="ALIGN_CENTER" width="20%">
								<g:Label text="F"/>
							</g:Cell>
						</g:HorizontalPanel>
						</g:Cell>
						<g:HorizontalPanel width="100%">
							<g:Cell width="20%" horizontalAlignment="ALIGN_CENTER">
								<g:SimpleCheckBox width="20"/>
							</g:Cell>
							<g:Cell width="20%" horizontalAlignment="ALIGN_CENTER">
								<g:SimpleCheckBox width="20"/>
							</g:Cell>
							<g:Cell width="20%" horizontalAlignment="ALIGN_CENTER">
								<g:SimpleCheckBox width="20"/>
							</g:Cell>
							<g:Cell width="20%" horizontalAlignment="ALIGN_CENTER">
								<g:SimpleCheckBox width="20"/>
							</g:Cell>
							<g:Cell width="20%" horizontalAlignment="ALIGN_CENTER">
								<g:SimpleCheckBox width="20"/>
							</g:Cell>
						</g:HorizontalPanel>
						
						<g:Label text="Start Time"/>
						<g:ListBox ui:field="startTimeSelect"/>
						<g:Label text="End Time"/>
						<g:ListBox ui:field="endTimeSelect"/>
						<g:Label text="Instructor"/>
						<g:ListBox ui:field="instructorSelect"/>
						<g:Cell horizontalAlignment="ALIGN_RIGHT">
							<g:Button>Delete</g:Button>
						</g:Cell>
					</g:VerticalPanel>
					</g:Cell>
					<g:Cell width="1%">
						<g:SimplePanel height="100%" width="1">
						<g:Label width="100%" height="100%" styleName="divider"/>
					</g:SimplePanel>
					</g:Cell>
					<g:Cell width="50%">
						<g:TabPanel ui:field="schedulePanel" width="100%" height="100%">
						<g:Tab text="Calendar">
							<g:ScrollPanel ui:field="calendarPanel" width="100%" height="100%">
								<g:Grid width="100%" height="100%">
									<g:row>
										<g:customCell>
											<g:Label text="Time" width="15%"/>
										</g:customCell>
										<g:customCell>
											<g:Label text="Monday" width="17%"/>
										</g:customCell>
										<g:customCell>
											<g:Label text="Tuesday" width="17%"/>
										</g:customCell>
										<g:customCell>
											<g:Label text="Wednesday" width="17%"/>
										</g:customCell>
										<g:customCell>
											<g:Label text="Thursday" width="17%"/>
										</g:customCell>
										<g:customCell>
											<g:Label text="Friday" width="17%"/>
										</g:customCell>
									</g:row>
								</g:Grid>
							</g:ScrollPanel>
						</g:Tab>
						<g:Tab text="Table">
							<g:ScrollPanel width="100%" height="100%" ui:field="tablePanel"/>
						</g:Tab>
					</g:TabPanel>
					</g:Cell>
					<g:Cell width="1%">
						<g:SimplePanel height="100%" width="1">
						<g:Label width="100%" height="100%" styleName="divider"/>
					</g:SimplePanel>
					</g:Cell>
					<g:Cell width="19%">
						<g:VerticalPanel ui:field="requestsMainPanel" width="100%">
						<g:Label text="Scheduling Requests"/>
						<g:ScrollPanel ui:field="requestsScrollPanel">
							<g:VerticalPanel width="100%" height="100%" ui:field="requestsPanel"/>
						</g:ScrollPanel>
					</g:VerticalPanel>
					</g:Cell>
				</g:HorizontalPanel>
			</g:Cell>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 